<a href="{{getLink}}" class="app_link bottom_line {{getType}}">
    <div>
        <img src="{{getImage}}" class="app_image" alt="image">
    </div>

    <span class="app_name_row">
        {{# getIcon }}
            <img src="{{ . }}" class="app_icon" alt="icon">
        {{/getIcon}}
        <span class="app_name">{{getName}} для <b>{{getPlatform}}</b></span>
    </span>
</a>
